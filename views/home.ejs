<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="/css/index.css" rel="stylesheet" type="text/css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
   
    <title>Document</title>
</head>
<body>
    <div class="container">
      <div id="root">
        <!-- navbar icon -->
      <nav class="navbar">
       <div><a href="http://localhost:8000/calender1"> <img id="menu" src="https://cdn-icons-png.flaticon.com/128/7630/7630204.png" alt=""></a>
        
    </div>
    <div><h3 id="today">Today Completed Task</h3></div>
    <div><a href="http://localhost:8000/calender1"><img id="rmenu" src="https://cdn-icons-png.flaticon.com/128/2550/2550264.png" alt=""></a></div>
    </nav>
    <!-- using express to show the data in page -->
    <% for (let i of contact_list) { %> 
        <div i class="list">
            <div id="work">
                <p id="text"><%= i.list %></p>  <img id="run" src="https://cdn-icons-png.flaticon.com/128/7192/7192355.png" alt=""></p> 
                <p id="time"><%= i.time %> <%= i.time1 %></p>

            </div>
        
            <div id="check">
                <input type="checkbox" name="" id="checkbox1" value="" checked>  <i onclick="myFunction(this)" class="fa fa-thumbs-up"></i> 
            </div>
            <div id="day">
                <p>1 Day</p>
                <p>9 best</p>
                <p>38/82</p>
                <p>Daily</p>
        
            </div>
            <p id="line"></p>
        
    <% } %>
      </div>
      
    </div>
    

    <script src="/js/index.js"></script>
  
</body>
</html>